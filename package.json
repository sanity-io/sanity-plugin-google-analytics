    "analytics",
    "google-analytics",
    "sanity",
    "sanity-plugin"
  ],
  "homepage": "https://github.com/sanity-io/sanity-plugin-google-analytics#readme",
  "bugs": {
    "url": "https://github.com/sanity-io/sanity-plugin-google-analytics/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:sanity-io/sanity-plugin-google-analytics.git"
  },
  "license": "ISC",
  "author": "Sanity.io <hello@sanity.io>",
  "type": "module",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "source": "./src/index.ts",
      "require": "./lib/index.cjs",
      "node": {
        "import": "./lib/index.cjs.js",
        "require": "./lib/index.cjs"
      },
      "import": "./lib/index.js",
      "default": "./lib/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./lib/index.cjs",
  "module": "./lib/index.js",
  "source": "./src/index.ts",
